<!--start static advertisement - long term-->
<!--Gallery image-->
<hr><br />
<div class="container" ng-controller="CMSController as cmsCtrl" ng-init="getPageContent('home_page')">
    <div class="row">
        <div data-js-module="filtering-demo">
            <div class="grid-view less-margin">
                <div class="col-sm-6 col-md-3 col-xs-6    transition hotels" data-category="transition" ng-repeat="gallery in content | filter: {contenttype:'gallery_image'}">
                    <div class="img-box">
                        <div class="pro-img" ng-show="gallery.param.filePath">
                            <img ng-src="{{gallery.param.filePath}}" alt="hotels" class="img-responsive">
                            <div class="overflow-outer">
                                <div class="overflow-inner">
                                    <h3>{{gallery.param.title}}</h3>
                                    <p>{{gallery.param.details}}</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!--end static advertisement - long term-->
<div class="our-work-wrapper" ng-controller="CMSController as cmsCtrl" ng-init="getPageContent('home_page')">
    <div class="container">
        <div class="row">
            <div class="col-md-4 col-sm-6" ng-repeat="extLink in content | filter: {contenttype:'external_link'}">
                <div class="our-work ">
                    <div class="hi-icon-wrap hi-icon-effect-5 hi-icon-effect-5a">
                        <img ng-show="extLink.param.filePath" ng-src="{{extLink.param.filePath}}">
                        <!--<span class="hi-icon hi-icon-images"></span>-->
                    </div>
                    <h4>{{extLink.param.title}}</h4>
                    <p>{{extLink.param.content}}</p>
                    <a ng-href="{{extLink.param.urlLink}}" class="btn btn-read-more">READ MORE</a>
                </div>
            </div>

            <!--end our-work-->
        </div>
    </div>
</div>

@section footer{
    <div class="container" ng-controller="CMSController as cmsCtrl" ng-init="getPageContent('contact_us')">
        <div class="row">
            <div class="col-md-3 col-sm-6 col-xs-12" ng-repeat="contact in content | filter: {contenttype:'contact_content'}">
                <div class="inner-footer1">
                    <h4>Need Help?</h4>
                    <!--<a href="index.html"><img src="assets/img/logo.png" alt="logo" class="img-responsive"></a>-->
                    <p class="footer-content">
                        {{contact.param.address1}} <br>
                        {{contact.param.address2}} <br>
                        {{contact.param.address3}}
                    </p>
                    <br>
                    <p class="footer-text"><span><a href="#"><i class="fa fa-phone"></i></a></span> Call center {{contact.param.callcenter}}<br>(Operation Time : 0830 - 1730)</p>
                    <p class="footer-text"><span><a href="#"><i class="fa fa-envelope"></i></a></span>  Email {{contact.param.email}}</p>
                    <div class="inner-footer" ng-controller="CMSController as cmsCtrl" ng-init="getPageContent('other_urlLink')">
                        <ul class="list-unstyled list-inline" ng-repeat="url in content | filter: {contenttype:'url_content'}">
                            <li>
                                <a href="/user/login"> <span class="social-icon"><i class="fa fa-sign-in"></i></span></a>
                            </li>
                            <li>
                                <a href="/portal/faq"> <span class="social-icon"><i class="fa fa-question-circle"></i></span></a>
                            </li>
                            <li>
                                <a ng-href={{url.param.facebook}}> <span class="social-icon"><i class="fa fa-facebook-f"></i></span></a>
                            </li>
                            <li>
                                <a ng-href={{url.param.twitter}}> <span class="social-icon"><i class="fa fa-twitter"></i></span></a>
                            </li>
                            <li>
                                <a ng-href={{url.param.instagram}}> <span class="social-icon"><i class="fa fa-instagram"></i></span></a>
                            </li>

                        </ul>
                    </div>

                </div>
            </div>
            <div class="col-md-3 col-sm-6 col-xs-12">
                <div class="inner-footer1">
                    <h4>Customer Service</h4>
                    <p class="footer-content">
                        Ask your questions away and let our staff help you look through our FAQs to get your questions answered!<br><br>
                    </p>

                    <form class="search-mail">
                        <input class="search-input" placeholder="Your question?">
                        <input class="search-Button" type="submit">
                    </form>
                </div>
            </div>
            <div class="col-md-3 hidden-sm hidden-xs col-xs-12" ng-controller="CMSController as cmsCtrl" ng-init="getPageContent('other_urlLink')">
                <div class="list-of-nav">
                    <div class="inner-footer1" ng-repeat="url in content | filter: {contenttype:'url_content'}">
                        <h4>About Us</h4>
                        <ul class="list-unstyled">
                            <li><a href="/portal/corporateprofile"> Corporate Profile</a></li>
                            <li><a href="/portal/contactus"> Contact Us</a></li>
                            <li><a href="/portal/ourservices"> Our Services</a></li>
                            <li><a href="/portal/mediarelease"> Media Release</a></li>
                            <li><a href="/portal/tender"> Tender & Quotations</a></li>
                            <li><a ng-href={{url.param.staffDashboard}}> Staff Dashboard</a></li>
                            <li><a href="/portal/faq"> FAQ</a></li>
                            @*<li><a href="#"> Travel With Us</a></li>
                                <li><a href="#"> Great Deals</a></li>
                                <li><a href="#"> Business Opportunities</a></li>*@
                        </ul>
                    </div>
                </div>
            </div>

            <div class="col-md-3 hidden-sm hidden-xs col-xs-12" ng-controller="CMSController as cmsCtrl" ng-init="getPageContent('news')">
                <div class="inner-footer2">
                    <h4>Latest News</h4>
                    <ul class="list-inline list-unstyled">
                        <li ng-repeat="news in content | filter: {contenttype:'news_content'}">
                            <div class="content-news" >
                                <p><a target="_blank" ng-href="{{news.param.filePath}}">{{news.param.headline}} </a></p>
                                <small>{{news.param.date}} </small>
                            </div>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
}

