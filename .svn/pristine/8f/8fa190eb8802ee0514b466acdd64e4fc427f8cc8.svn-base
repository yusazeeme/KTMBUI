
@{
    ViewBag.Title = "Select";
}

<style>
    .hover_img a {
        position: relative;
    }

        .hover_img a span {
            position: absolute;
            display: none;
            z-index: 99;
        }

        .hover_img a:hover span {
            display: block;
        }
        .cursor {
            cursor: pointer;
        }
</style>
<!-- Scrolling Tab CSS -->
<link rel="stylesheet" href="~/Content/assets/js/scrollingtabs/jquery.scrolling-tabs.css">

<div class="container" ng-controller="TicketController as ticketCtrl" ng-init="getCoachV2(); checkQueue();">
    <div class="row" ng-controller="LookupController as lookupCtrl" ng-init="generateTripDate();journey = {};">
        <div class="col-md-12" style="margin:5% 0; margin-bottom:10%;">
            <div class="form-bootstrapWizard">
                <ul class="bootstrapWizard form-wizard">
                    <li>
                        <a href="{{UIURL}}" class="active"> <span class="step">1</span> <span class="title">Search Train</span> </a>
                    </li>
                    <li class="active">
                        <a href="#"> <span class="step">2</span> <span class="title">Select Train</span> </a>
                    </li>
                    <li>
                        <a href="#"> <span class="step">3</span> <span class="title">Passenger Info</span> </a>
                    </li>
                    <li>
                        <a href="#"> <span class="step">4</span> <span class="title">Select Seat</span> </a>
                    </li>
                    <li>
                        <a href="#"> <span class="step">5</span> <span class="title">Payment</span> </a>
                    </li>
                </ul>
                <div class="clearfix"></div>
            </div>
        </div>

        <!--wajib ada-->
        <header></header>
        <div class="panel-body">
            <div style="float:right;">
                <a href="#" data-toggle="modal" data-target="#modal-2" class="btn btn-default" ng-clcik="getSearchTrain()">Edit Search</a>
            </div>
        </div>

        <!--Start Departure-->
        <div class="row" style="margin-top: -25px; padding-top: 105px;">
            <form ng-submit="proceedBooking()">
                <div class="col-md-12">
                    <!--start content and scrolling-->
                    <main class="clearfix">
                        <!--start sidebar-->
                        <aside id="sidebar" style="margin-top:-1%;">
                            <div class="inside">
                                <div class="flight_detail_sidebar white-box" style="margin-top:37%;">
                                    <div class="details-text">
                                        <h4>
                                            Journey Summary
                                            <br />
                                            <small ng-show="!returnList.length"> (One Way)</small>
                                            <small ng-show="returnList.length"> (Return)</small>
                                            <br />
                                        </h4>
                                    </div>
                                    <div class="railway-font">
                                        <small>GRAND TOTAL (MYR)</small>
                                        <div class="alert alert-info"><h2 class="price-info">{{totalCost}}</h2></div>
                                    </div>

                                    <div ng-repeat="st in selectedTrip | orderBy :'+DepartureTime'">
                                        <hr />
                                        <div class="title-direction">
                                            <h4>{{st.OriginName}} - {{st.DestinationName}}</h4>
                                        </div>
                                        @*<div class="LTT">
                                            <span class="skin-clr">
                                            Fare:
                                            <span class="pull-right" ng-show="st.TotalAdult != '0'">
                                            Adult : {{st.TotalAdult}} x {{st.SeatFareAdult | number : 2}} MYR
                                            </span>
                                            <span class="pull-right" ng-show="st.TotalChild != '0'">
                                            Child : {{st.TotalChild}} x {{st.SeatFareChild | number : 2}} MYR
                                            </span>
                                            </span>
                                            </div>*@
                                        <span class="skin-clr">
                                            Fare:
                                            <span class="pull-right" ng-show="st.TotalAdult != '0'">
                                                Adult : {{st.TotalAdult}} x {{st.SeatFareAdult.replace('MYR ', '') | number : 2}} MYR
                                            </span>
                                            <span class="pull-right" ng-show="st.TotalChild != '0'">
                                                Child : {{st.TotalChild}} x {{st.SeatFareChild.replace('MYR ', '') | number : 2}} MYR
                                            </span>
                                        </span>
                                        @*<div class="LTT">
                                            <span class="skin-clr">
                                            Admin Charge
                                            <span class="pull-right">
                                            {{st.TotalPassenger}} x {{st.SingleCharge}} MYR
                                            </span>
                                            </span>
                                            </div>*@
                                        <div class="LTT">
                                            <span class="skin-clr">
                                                Total
                                                <span class="pull-right">
                                                    {{st.NormalPrice.replace('MYR', '')}} MYR
                                                </span>
                                            </span>
                                        </div>
                                        @*<div class="LTT">
                                            <small>*price inclusive with GST</small>
                                            </div>*@
                                    </div>

                                    <hr />
                                    <div class="railway-font">
                                        <input type="submit" class="btn btn-proceed-travel btn-lg" value="PROCEED &raquo;" ng-disabled="!readyToProceed" />
                                    </div>
                                </div>
                            </div>
                        </aside>
                        <!--end sidebar-->
                        <!-- start main content-->
                        <div class="main_content" id="content" ng-init="waitingTime()">
                            <div id="tabbing">
                                <ul class="nav-tabs nav" role="tablist">
                                    <li role="presentation"
                                        ng-repeat="onwardDate in OnwardTripDateList | orderBy :'+tripDate'"
                                        ng-class="{'active':onwardDate.isactive === true}">
                                        <a href="#tab4" role="tab" data-toggle="tab" class="cursor" ng-click="changeTravellingDate(false, onwardDate.tripDate)">
                                            {{onwardDate.tripDate | date:'dd MMM, EEE'}}
                                        </a>
                                    </li>
                                </ul>
                            </div>
                            <div class="tab-content" ng-repeat="owl in onwardList">
                                <!---!!!!!! id="tab1" dapat data dari atas-->
                                <div role="tabpanel" class="tab-pane active" id="tab4">
                                    <!--start depart-->
                                    <div class="bread-crumb animate-reveal change-position" style="background-color: #ffe900;">
                                        <h2 style="padding:20px;">{{owl.OriginName}} - {{owl.DestinationName}}</h2>
                                    </div>
                                    <div class="flight_box_detail white-box animate-reveal tengah" ng-show="!owl.TripList.length && stopwaiting">
                                        Please wait...
                                    </div>
                                    <div class="flight_box_detail white-box animate-reveal tengah" ng-show="!owl.TripList.length && !owl.TripList == '0' && !stopwaiting">
                                        Sorry, the train is fully booked. Please select other date.
                                    </div>
                                    <div class="flight_box_detail white-box animate-reveal tengah" ng-show="!owl.TripList && !stopwaiting">
                                        Sorry, train is NOT yet schedule. Please select other date.
                                    </div>

                                    <div class="flight_box_detail white-box animate-reveal tengah" ng-repeat="trip in owl.TripList | orderBy :'+DepartureTime'">
                                        <div class="row">
                                            <div class="col-md-12">
                                                <div class="row">
                                                    <div class="col-md-4">
                                                        <div class="details-text">
                                                            <br>
                                                            <h4>
                                                                {{trip.TrainName}}<br ng-if="trip.HasLowFare" />
                                                                <a href="#" class="btn btn-stop" ng-show="trip.HasLowFare">low fare</a>
                                                                <br />
                                                                <small>({{trip.TrainNumber}})</small> - <small>({{trip.CoachName}})</small>
                                                                @*<span class="hover_img">
                                                                    <a href="#">
                                                                    <i class="fa fa-camera-retro"></i>
                                                                    <span><img ng-src="{{trip.PicURL}}" alt="image" /></span>
                                                                    </a>
                                                                    </span>*@
                                                            </h4>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-8">
                                                        <p style="color:red;" ng-show="!trip.CanBook">Departure time less than 4 hours, please proceed to counter to purchase the ticket.</p>
                                                        <div class="select-sec" style="margin-bottom: 10px;">
                                                            <input type="radio" class="cursor" name="{{owl.OriginCode}}_{{owl.DestinationCode}}_{{owl.Sequence}}" id="{{trip.TempId}}" value="{{trip.TempId}}" ng-model="$parent.selectedtrip" ng-change='bookingOnClick(trip.TempId, false)' ng-disabled="!trip.IsShow || !trip.CanBook">
                                                            <label>
                                                                @*<div class="select_title">{{trip.Currency}} {{trip.SeatFareAdult | number : 2}} (Adult)</div>
                                                                    <div class="select_title" ng-show="trip.TotalChild!=0">{{trip.Currency}} {{trip.SeatFareChild | number : 2}} (Child)</div>*@
                                                                <div class="select_title">{{trip.SeatFareAdult}} (Adult)</div>
                                                                <div class="select_title" ng-show="trip.TotalChild!=0">{{trip.SeatFareChild}} (Child)</div>
                                                                <div class="select_seat" ng-show="!trip.CoachCode.includes('ADN')">{{trip.NormalSeatLeft}} seat(s) left</div>
                                                                <div class="select_seat" ng-show="trip.CoachCode.includes('ADN')">{{trip.NormalSeatLeft}} berth(s) left</div>
                                                            </label>
                                                        </div>
                                                    </div>
                                                </div>
                                                <hr>
                                                <div class="row">
                                                    <div class="col-md-4">
                                                        <div class="LTT">
                                                            <span class="skin-clr"> <i class="fa fa-clock-o"></i> Duration</span><br>
                                                            <span class="time">{{trip.Duration}}</span>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <div class="LTT">
                                                            <span class="skin-clr"> <i class="fa fa-subway"></i><i class="fa fa-share"></i> Departure</span><br>
                                                            <span class="time">{{trip.DepartureTime}}</span>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <div class="LTT no-border">
                                                            <span class="skin-clr"> <i class="fa fa-subway"></i><i class="fa fa-flag-checkered"></i> Arrival</span><br>
                                                            <span class="time">{{trip.ArrivalTime}}</span>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <!--end depart-->
                                </div>
                            </div>
                            <div class="col-sm-12">
                                <div style="margin-top:6%">&nbsp;</div>
                            </div>

                            <!--start return-->
                            <div id="tabbing" class="return-date">
                                <ul class="nav-tabs nav" role="tablist">
                                    <li role="presentation"
                                        ng-repeat="returnDate in ReturnTripDateList | orderBy :'+tripDate'"
                                        ng-class="{'active':returnDate.isactive === true}">
                                        <a href="#tab4" role="tab" data-toggle="tab" class="cursor" ng-click="changeTravellingDate(true, returnDate.tripDate)">
                                            {{returnDate.tripDate | date:'dd MMM, EEE'}}
                                        </a>
                                    </li>
                                </ul>
                            </div>
                            <div class="tab-content" ng-repeat="rl in returnList">
                                <!---!!!!!! id="tab1" dapat data dari atas-->
                                <div role="tabpanel" class="tab-pane active" id="tab5">
                                    <div class="bread-crumb animate-reveal change-position" style="background-color:#0053a0;">
                                        <!--Return tukar color-->
                                        <h2 style="color:#ffe900; padding:20px;">{{rl.OriginName}} - {{rl.DestinationName}}</h2>
                                    </div>

                                    <div class="flight_box_detail white-box animate-reveal tengah" ng-show="!selectedTrip.length" style="color:white; font-size:18px; background-color:red;">
                                        Please select onward train first...
                                    </div>
                                    <div ng-show="selectedTrip.length">
                                        <div class="flight_box_detail white-box animate-reveal tengah" ng-show="!rl.TripList.length && stopwaiting">
                                            Please wait...
                                        </div>
                                        <div class="flight_box_detail white-box animate-reveal tengah" ng-show="!rl.TripList.length && !rl.TripList == '0' && !stopwaiting">
                                            Sorry, the train is fully booked. Please select other date.
                                        </div>
                                        <div class="flight_box_detail white-box animate-reveal tengah" ng-show="!rl.TripList && !stopwaiting">
                                            Sorry, train is NOT yet schedule. Please select other date.
                                        </div>

                                        <div class="flight_box_detail white-box animate-reveal tengah" ng-repeat="trip in rl.TripList | orderBy :'+DepartureTime'">
                                            <div class="row">
                                                <div class="col-md-12">
                                                    <div class="row">
                                                        <div class="col-md-4">
                                                            <div class="details-text">
                                                                <br>
                                                                <h4>
                                                                    {{trip.TrainName}}<br ng-if="trip.HasLowFare" />
                                                                    <a href="#" class="btn btn-stop" ng-show="trip.HasLowFare">low fare</a>
                                                                    <br />
                                                                    <small>({{trip.TrainNumber}})</small> - <small>({{trip.CoachName}})</small>
                                                                    @*<span class="hover_img">
                                                                        <a href="#">
                                                                        <i class="fa fa-camera-retro"></i>
                                                                        <span><img ng-src="{{trip.PicURL}}" alt="image" /></span>
                                                                        </a>
                                                                        </span>*@
                                                                </h4>
                                                            </div>
                                                        </div>
                                                        <div class="col-md-8">
                                                            <p style="color:red;" ng-show="!trip.CanBook">Departure time less than 4 hours, please proceed to counter to purchase the ticket.</p>
                                                            <div class="select-sec" style="margin-bottom: 10px;">
                                                                <input type="radio" class="cursor" name="{{rl.OriginCode}}_{{rl.DestinationCode}}_{{rl.Sequence}}" id="{{trip.TempId}}" value="{{trip.TempId}}" ng-model="$parent.selectedtrip" ng-change='bookingOnClick(trip.TempId, false)' ng-disabled="!trip.IsShow || !trip.CanBook">
                                                                <label>
                                                                    @*<div class="select_title">{{trip.Currency}} {{trip.SeatFareAdult | number : 2}} (Adult)</div>
                                                                        <div class="select_title" ng-show="trip.TotalChild!=0">{{trip.Currency}} {{trip.SeatFareChild | number : 2}} (Child)</div>*@
                                                                    <div class="select_title">{{trip.SeatFareAdult}} (Adult)</div>
                                                                    <div class="select_title" ng-show="trip.TotalChild!=0">{{trip.SeatFareChild}} (Child)</div>
                                                                    <div class="select_seat" ng-show="!trip.CoachCode.includes('ADN')">{{trip.NormalSeatLeft}} seat(s) left</div>
                                                                    <div class="select_seat" ng-show="trip.CoachCode.includes('ADN')">{{trip.NormalSeatLeft}} berth(s) left</div>
                                                                </label>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <hr>
                                                    <div class="row">
                                                        <div class="col-md-4">
                                                            <div class="LTT">
                                                                <span class="skin-clr"> <i class="fa fa-clock-o"></i> Duration</span><br>
                                                                <span class="time">{{trip.Duration}}</span>
                                                            </div>
                                                        </div>
                                                        <div class="col-md-4">
                                                            <div class="LTT">
                                                                <span class="skin-clr"> <i class="fa fa-subway"></i><i class="fa fa-share"></i> Departure</span><br>
                                                                <span class="time">{{trip.DepartureTime}}</span>
                                                            </div>
                                                        </div>
                                                        <div class="col-md-4">
                                                            <div class="LTT no-border">
                                                                <span class="skin-clr"> <i class="fa fa-subway"></i><i class="fa fa-flag-checkered"></i> Arrival</span><br>
                                                                <span class="time">{{trip.ArrivalTime}}</span>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <!--end depart-->
                                </div>
                            </div>
                            <!--end return-->
                        </div>
                        <!-- end main content-->

                    </main>
                    <!--end content and scrolling-->
                    <!--End Departure-->
                </div>
                <!--End Departure-->
            </form>
        </div>

        <!-- Modal 2 (Basic)-->
        <div class="modal fade" id="modal-2">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                        <h4 class="modal-title"></h4>
                    </div>
                    <div class="panel panel-primary" data-collapsed="0">
                        <div class="panel-body">
                            <div class="wrapper-inner2">
                                <!--text-box-->
                                <!--main-fill-->
                                @*<div>*@
                                <div class="main-fill2">
                                    <!--tabs-->
                                    <div class="panel">
                                        <div class="panel-heading" style="padding:4px 10px 4px 10px;">
                                            @*<div id="searching">
                                                    <ul class="nav nav-tabs">
                                                        <li class="active"><a href="#taboneway" data-toggle="tab"><div class="icon"></div>&nbsp;<i class="fa fa-train"></i> One Way <div class="icon2"></div></a></li>
                                                        <li><a href="#tabreturn" data-toggle="tab"><div class="icon"></div>&nbsp; <i class="fa fa-train"></i> Return <div class="icon2"></div>&nbsp;</a></li>
                                                    </ul>
                                                </div>*@
                                            <a href="#taboneway" data-toggle="tab" data-target="#taboneway" class="btn btn-default">One Way</a>
                                            <a href="#tabreturn" data-toggle="tab" data-target="#tabreturn" class="btn btn-default">Return</a>

                                        </div>
                                        <div class="panel-body">
                                            <!--tab-content-->
                                            <div class="tab-content">
                                                <!--start taboneway-->
                                                <div class="tab-pane fade in active" id="taboneway">
                                                    <form ng-submit="searchTrain(journey, '0')">
                                                        <ul class="list-inline list-unstyled">
                                                            <li style="width:50%;">
                                                                <select class="select-one col-md-3" ng-init="getOriginList()"
                                                                        ng-model="journey.origin"
                                                                        ng-options="destination.CODE as destination.NAME group by destination.STATE for destination in origins" required>
                                                                    <option value=''>Origin</option>
                                                                </select>
                                                            </li>
                                                            <li style="width:50%;">
                                                                <select class="select-one col-md-3" ng-init="getDestinationList(journey.origin)"
                                                                        ng-model="journey.destination"
                                                                        ng-options="destination.CODE as destination.NAME group by destination.STATE for destination in destinations" required>
                                                                    <option value=''>Destination</option>
                                                                </select>
                                                            </li>
                                                            <li style="width:50%;">
                                                                <div class="form-group" ng-controller="DateController">
                                                                    <input type="text" style="color:#2c2c2c;" class="form-control" placeholder="Departure Date" uib-datepicker-popup="dd/MM/yyyy" ng-click="open1()" ng-model="journey.departuredate" is-open="popup1.opened" datepicker-options="dateOptions" ng-required="true" close-text="Close" alt-input-formats="altInputFormats" ng-change="setMinDate()" />
                                                                </div>
                                                            </li>
                                                            <li style="width:50%;">
                                                                <select class="select-one col-md-3" ng-model="journey.departureth" required>
                                                                    <option value=''>Departure Time Range</option>
                                                                    <option value='EM'>12:00 AM - 06:59 AM</option>
                                                                    <option value='MO'>07:00 AM - 11:59 AM</option>
                                                                    <option value='NO'>12:00 PM - 06:59 PM</option>
                                                                    <option value='NI'>07:00 PM - 11:59 PM</option>
                                                                </select>
                                                            </li>
                                                        </ul>
                                                        <div class="clearfix"></div>
                                                        <!--<h3>Who's Travelling?</h3>-->
                                                        <ul class="list-inline list-unstyled">
                                                            <li style="width:50%;">
                                                                <select class="select-one" style="margin-bottom: 5px;" ng-model="journey.noadult" required>
                                                                    <option value="0">0 Adult</option>
                                                                    <option value="1">1 Adult</option>
                                                                    <option value="2">2 Adults</option>
                                                                    <option value="3">3 Adults</option>
                                                                    <option value="4">4 Adults</option>
                                                                    <option value="5">5 Adults</option>
                                                                    <option value="6">6 Adults</option>
                                                                </select>
                                                            </li>
                                                            <li style="width:50%;">
                                                                <select class="select-one" style="margin-bottom: 5px;" ng-model="journey.nochild" required>
                                                                    <option value="0">0 Child</option>
                                                                    <option value="1">1 Child</option>
                                                                    <option value="2">2 Children</option>
                                                                    <option value="3">3 Children</option>
                                                                    <option value="4">4 Children</option>
                                                                    <option value="5">5 Children</option>
                                                                    <option value="6">6 Children</option>
                                                                </select>
                                                                <div class="i_note">4 - 12 years</div>
                                                            </li>
                                                            <li style="width:50%;">
                                                                <input type="submit" class="btn btn-search-travel" ng-disabled="journey.nochild == '0' && journey.noadult== '0'" value="Search" />

                                                            </li>
                                                            <li style="width:50%;">
                                                                <button class="btn btn-search-travel" ng-click="journey={}; journey.nochild='0'; journey.noadult='1';">Clear</button>
                                                            </li>
                                                        </ul>
                                                    </form>
                                                </div>
                                                <!--end taboneway-->
                                                <!--start tabreturn-->
                                                <div class="tab-pane fade in" id="tabreturn">
                                                    <form ng-submit="searchTrain(journey, '1')">
                                                        <ul class="list-inline list-unstyled" ng-controller="DateController">
                                                            <li style="width:50%;">
                                                                <select class="select-one col-md-3"
                                                                        ng-model="journey.origin" ng-mouseover="getOriginList()"
                                                                        ng-options="destination.CODE as destination.NAME group by destination.STATE for destination in origins" ng-change="getDestinationList(journey.origin)" required>
                                                                    <option value=''>Origin</option>
                                                                </select>
                                                            </li>
                                                            <li style="width:50%;">
                                                                <select class="select-one col-md-3"
                                                                        ng-model="journey.destination"
                                                                        ng-options="destination.CODE as destination.NAME group by destination.STATE for destination in destinations" required>
                                                                    <option value=''>Destination</option>
                                                                </select>
                                                            </li>
                                                            <li style="width:50%;">
                                                                <div class="form-group">
                                                                    <input type="text" class="form-control" placeholder="Departure Date" uib-datepicker-popup="dd/MM/yyyy" ng-click="open1()" ng-model="journey.departuredate" is-open="popup1.opened" datepicker-options="dateOptions" ng-required="true" close-text="Close" alt-input-formats="altInputFormats" ng-change="setMinDate()" />
                                                                </div>
                                                            </li>
                                                            <li style="width:50%;" class="">
                                                                <select class="select-one col-md-3" ng-model="journey.departureth" required>
                                                                    <option value=''>Departure Time Range</option>
                                                                    <option value='EM'>12:00 AM - 06:59 AM</option>
                                                                    <option value='MO'>07:00 AM - 11:59 AM</option>
                                                                    <option value='NO'>12:00 PM - 06:59 PM</option>
                                                                    <option value='NI'>07:00 PM - 11:59 PM</option>
                                                                </select>
                                                            </li>
                                                            <li style="width:50%;">
                                                                <div class="form-group">
                                                                    <input type="text" class="form-control" placeholder="Return Date" uib-datepicker-popup="dd/MM/yyyy" ng-click="open2()" ng-model="journey.returndate" is-open="popup2.opened" datepicker-options="{minDate: journey.departuredate}" ng-required="true" close-text="Close" alt-input-formats="altInputFormats" />
                                                                </div>
                                                            </li>
                                                            <li style="width:50%;" class="">
                                                                <select class="select-one col-md-3" ng-model="journey.returnth" required>
                                                                    <option value=''>Return Time Range</option>
                                                                    <option value='EM'>12:00 AM - 06:59 AM</option>
                                                                    <option value='MO'>07:00 AM - 11:59 AM</option>
                                                                    <option value='NO'>12:00 PM - 06:59 PM</option>
                                                                    <option value='NI'>07:00 PM - 11:59 PM</option>
                                                                </select>
                                                            </li>
                                                        </ul>
                                                        <div class="clearfix"></div>
                                                        <ul class="list-inline list-unstyled">
                                                            <li style="width:50%;">
                                                                <select class="select-one" style="margin-bottom: 5px;" ng-model="journey.noadult" ng-init="journey.noadult='1'" required>
                                                                    <option value="0">0 Adult</option>
                                                                    <option value="1">1 Adult</option>
                                                                    <option value="2">2 Adults</option>
