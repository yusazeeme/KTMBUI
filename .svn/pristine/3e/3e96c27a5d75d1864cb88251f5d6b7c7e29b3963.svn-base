                            <ul class="dropdown-menu" ng-show="menu.SubMenu.length && menu.SubMenu.Menu.url == ''">
                                <li ng-repeat="subMenu in menu.SubMenu"><a class="sizefont" href="#">{{subMenu.Menu.title_en}}</a></li>
                            </ul>
                        </li>
