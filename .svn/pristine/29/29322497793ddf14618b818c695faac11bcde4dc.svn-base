
@{
    ViewBag.Title = "Contact Us";
    Layout = "~/Views/Shared/_CMSLayout.cshtml";
}
<div ng-controller="CMSController as cmsCtrl">
    <ol class="breadcrumb bc-3">
        <li>
            <a href="index.html"><i class="fa-home"></i>Home</a>
        </li>
        <li>
            <a href="forms-main.html">Forms</a>
        </li>
        <li class="active">
            <strong>Contact Us</strong>
        </li>
    </ol>
    <h2>Contact Us</h2>
    <br />

    <div class="row" ng-init="getCMS('contact_us', 'contact_content', true)">
        <div class="col-md-12">
            <div class="panel panel-primary" data-collapsed="0">
                <div class="panel-heading">
                    <div class="panel-title">
                        Contact Us
                    </div>
                </div>

                <div class="panel-body">
                    <form role="form" class="form-horizontal form-groups-bordered" ng-submit="createCMS(contact_content, 'contact_us', 'contact_content', true, false)">
                        <div class="form-group">
                            <label for="field-1" class="col-sm-3 control-label">Address 1</label>
                            <div class="col-sm-5">
                                <input type="text" class="form-control" ng-model="contact_content.param.address1" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="field-1" class="col-sm-3 control-label">Address 2</label>
                            <div class="col-sm-5">
                                <input type="text" class="form-control" ng-model="contact_content.param.address2" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="field-1" class="col-sm-3 control-label">Address 3</label>

                            <div class="col-sm-5">
                                <input type="text" class="form-control" ng-model="contact_content.param.address3" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="field-1" class="col-sm-3 control-label">Phone Office</label>

                            <div class="col-sm-5">
                                <input type="text" class="form-control" ng-model="contact_content.param.phoneNo" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="field-1" class="col-sm-3 control-label">Call Center</label>

                            <div class="col-sm-5">
                                <input type="text" class="form-control" ng-model="contact_content.param.callcenter" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="field-1" class="col-sm-3 control-label">Email Address</label>

                            <div class="col-sm-5">
                                <input type="email" class="form-control" ng-model="contact_content.param.email" />
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-sm-offset-3 col-sm-5">
                                <button type="submit" class="btn btn-default">Save</button>
                            </div>
                        </div>
                    </form>
                    <hr />
                </div>
            </div>
        </div>
    </div>
</div>


<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCyW6EPDfjtmGMbkVULLP13ndpQ2Nnuh88"></script>


