
@{
    ViewBag.Title = "TravelWithUs";
    Layout = "~/Views/Shared/_CMSLayout.cshtml";
}

<div ng-controller="UserController as userCtrl" ng-init="checkAccess(true)"></div>
<ol class="breadcrumb bc-3">
    <li>
        <a href="/cms/index"><i class="fa-home"></i>Home</a>
    </li>
    <li class="active">

        <strong>Travel With Us</strong>
    </li>
</ol>

<h2>Travel With Us</h2>
<br />

<div class="row">
    <div class="col-md-12">

        <div class="panel panel-primary" data-collapsed="0">

            <div class="panel-heading">
                <div class="panel-title">
                    Default Form Inputs
                </div>

            </div>

            <div class="panel-body">

                <form role="form" class="form-horizontal form-groups-bordered">

                    <div class="form-group">
                        <label for="field-1" class="col-sm-3 control-label">Title</label>

                        <div class="col-sm-5">
                            <input type="text" class="form-control" id="field-1" placeholder="Placeholder">
                        </div>
                    </div>

                    <div class="form-group">
                        <label for="field-1" class="col-sm-3 control-label">Content1</label>

                        <div class="col-sm-5">
                            <textarea class="form-control" id="field-ta" placeholder="Textarea"></textarea>
                        </div>
                    </div>

                    <div class="form-group">
                        <label for="field-1" class="col-sm-3 control-label">Content2</label>

                        <div class="col-sm-5">
                            <textarea class="form-control" id="field-ta" placeholder="Textarea"></textarea>
                        </div>
                    </div>

                    <div class="form-group">
                        <label for="field-1" class="col-sm-3 control-label">Mission Tile</label>

                        <div class="col-sm-5">
                            <input type="text" class="form-control" id="field-1" placeholder="Placeholder">
                        </div>
                    </div>

                    <div class="form-group">
                        <label for="field-1" class="col-sm-3 control-label">Mission Content</label>

                        <div class="col-sm-5">
                            <textarea class="form-control" id="field-ta" placeholder="Textarea"></textarea>
                        </div>
                    </div>

                    <div class="form-group">
                        <label for="field-1" class="col-sm-3 control-label">Vission Tile</label>

                        <div class="col-sm-5">
                            <input type="text" class="form-control" id="field-1" placeholder="Placeholder">
                        </div>
                    </div>

                    <div class="form-group">
                        <label for="field-1" class="col-sm-3 control-label">Vission Content</label>

                        <div class="col-sm-5">
                            <textarea class="form-control" id="field-ta" placeholder="Textarea"></textarea>
                        </div>
                    </div>

                    <div class="form-group">
                        <label for="field-1" class="col-sm-3 control-label">Board Title</label>

                        <div class="col-sm-5">
                            <input type="text" class="form-control" id="field-1" placeholder="Placeholder">
                        </div>
                    </div>

                    <div class="form-group">
                        <label for="field-1" class="col-sm-3 control-label">File Field</label>

                        <div class="col-sm-5">
                            <input type="file" class="form-control" id="field-file" placeholder="Placeholder">
                        </div>
                    </div>

                    <div class="form-group">
                        <div class="col-sm-offset-3 col-sm-5">
                            <button type="submit" class="btn btn-default">Submit</button>
                        </div>
                    </div>
                </form>

            </div>

        </div>

    </div>
</div>



