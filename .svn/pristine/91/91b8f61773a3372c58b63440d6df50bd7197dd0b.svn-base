	background: linear-gradient(to bottom,  #FFCC00 7%,#fdd513 23%,#fdff77 43%,#17aafa 60%,#0053a0 100%,#077ff1 100%,#036aca 100%,#0053a0 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#f9fc63', endColorstr='#3cedd9',GradientType=0 ); /* IE6-9 */
