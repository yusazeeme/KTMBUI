	background: -webkit-gradient(linear, left top, left bottom, color-stop(7%,#f9fc63), color-stop(23%,#d6f979), color-stop(43%,#a9f794), color-stop(60%,#59f0c7), color-stop(100%,#18ecef), color-stop(100%,#7db9e8), color-stop(100%,#04e9fc), color-stop(100%,#3cedd9)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top,  #FFCC00 7%,#fdd513 23%,#fdff77 43%,#17aafa 60%,#0053a0 100%,#077ff1 100%,#036aca 100%,#0053a0 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top,  #FFCC00 7%,#fdd513 23%,#fdff77 43%,#17aafa 60%,#0053a0 100%,#077ff1 100%,#036aca 100%,#0053a0 100%); /* Opera 11.10+ */
