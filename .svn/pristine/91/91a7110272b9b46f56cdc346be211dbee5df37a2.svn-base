#loading{
    background-color: $secondry-color;
    height: 100%;
    width: 100%;
    position: fixed;
    z-index: 10000;
    margin-top: 0px;
    top: 0px;
}
#loading-center{
    width: 100%;
    height: 100%;
    position: relative;
}
#loading-center-absolute {
    position: absolute;
    left:48%;
    top:45%;
    height: 150px;
    width: 150px;
    margin-top: -75px;
    margin-left: -75px;

}

#object_big{
    position: absolute;
    width: 100px;
    height: 100px;
    left: 0px;
    -webkit-animation: object_big 1s infinite;
    animation: object_big 1s infinite;
    -webkit-animation-delay: 0.2s;
    animation-delay: 0.2s;
    img{
        height: 200px;
    }
}

@-webkit-keyframes object_big {
    50% { -webkit-transform:  scale(0.5); }
}

@keyframes object_big {
    50% {
        transform: scale(1.5);
        -webkit-transform: scale(0.5);
    }
}









