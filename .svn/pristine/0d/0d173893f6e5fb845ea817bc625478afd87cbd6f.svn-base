                                $scope.ChildPriceBreakdown.push(pd);
                                x++;
                                GetChildPriceBreakdownStep4(trip, paxlist, x);
                            }
                        }).catch(function onError(response) {
                            console.log('ERROR!: ' + response.statusText + ' at line 1725, ticket.js');
                        });
                    }
                }
                else {
                    x++;
                    GetChildPriceBreakdownStep4(trip, paxlist, x);
