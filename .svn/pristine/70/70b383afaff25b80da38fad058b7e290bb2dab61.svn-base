.direction {
    margin: 0px;
    padding: 0;
}

.ringgit {
    margin: 0px;
    padding: 0;
    float: left !important;
    margin-top: 10px;
}

.ringgit_price {
    margin: 0px;
    padding: 0;
}

.direction_booking {
    margin: 0px;
    padding: 0;
}

.back_white {
    background-color: #ffffff;
}

.stepwizard-step p {
    margin-top: 10px;
}

.stepwizard-row {
    display: table-row;
}

.stepwizard {
    display: table;
    width: 50%;
    position: relative;
}

.stepwizard-step button[disabled] {
    opacity: 1 !important;
    filter: alpha(opacity=100) !important;
}

.stepwizard-row:before {
    top: 21px;
    bottom: 0;
    position: absolute;
    content: " ";
    width: 100%;
    height: 3px;
    background-color: #0053a0;
    z-order: 0;
}

.stepwizard-step {
    display: table-cell;
    text-align: center;
    position: relative;
}

.btn-circle {
    width: 35px;
    height: 35px;
    text-align: center;
    padding: 6px 0;
    font-size: 16px;
    line-height: 1.428571429;
    border-radius: 20px;
    font-weight: 700;
}

.btn-circle2 {
    width: 45px;
    height: 45px;
    text-align: center;
    padding: 6px 0;
    font-size: 23px;
    line-height: 1.428571429;
    border-radius: 40px;
    font-weight: 700;
}

.btn-step {
    color: #0053a0;
    background-color: #ffcc00;
    border-color: #ffcc00;
}

.btn-default2 {
    color: #333;
    background-color: #efefef;
    border-color: #dddddd;
}

.tengah {
    text-align: center !important;
}

.white {
    color: #ffffff !important;
}

.i_info {
    display: inline-block;
    color: #fff !important;
    position: absolute;
    float: right;
    margin: 4%;
}

.i_note {
    color: #ffffff;
    font-size: 11px;
}

.scroll-index3 {
    margin-left: 5px !important;
}

.big_25 {
    width: 25% !important;
}

.left-sided, .promo_train_depart, .promo_train_arrive, .promo_train_tagprice, .promo_train_price, .promo_train_journey_type {
    float: left;
    font-family: "Open Sans", sans-serif;
}

.promo_train_travel_date, .promo_train_promocode {
    float: right;
    font-family: "Open Sans", sans-serif;
}

.promo_train_depart, .promo_train_tagprice, .promo_train_journey_type {
    font-size: 13px;
}

.promo_train_arrive {
    font-size: 20px;
    font-weight: 400;
    display: block;
    padding-bottom: 1px;
