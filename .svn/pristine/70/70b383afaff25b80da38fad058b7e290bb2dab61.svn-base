}

.promo_train_tagprice, .promo_train_price {
    padding-top: 30px;
}

.promo_train_tagprice, .promo_train_price {
    position: absolute;
}

.promo_train_journey_type, .promo_train_travel_date {
    margin-top: 20%;
}

.index_v3_scroll {
    background-color: rgba(0, 83, 160, 1) !important;
    color: #ffffff !important;
}

.scroll-index3 .slick-prev, .scroll-index3 .slick-next {
    background-color: #0053a0 !important;
}

.flight_box_detail .select-sec {
    margin-bottom: 10px !important;
}

.railway-font {
    font-family: 'Raleway', sans-serif;
}

.price-info {
    margin: -8px 0px -2px 0px !important;
    text-align: center;
}

.flight_detail_sidebar .details-text .tengah {
    text-align: center !important;
}

.title-direction {
    float: left;
    text-align: left;
    position: relative;
    width: 100%;
}

    .title-direction h4 {
        position: relative;
        color: #636363;
        font-family: "Raleway", sans-serif;
        margin: 0 0 10px;
        text-transform: capitalize;
        font-weight: 600;
        font-size: 16px;
        text-transform: uppercase;
    }

        .title-direction h4 small {
            color: #797979;
            font-family: "Open Sans", sans-serif;
            font-weight: 600;
        }

.showed {
    transition-duration: .9s;
    border-radius: 100%;
    position: absolute;
    z-index: 999;
    margin-bottom: -10%;
    background: transparent;
    border-color: transparent;
}

.hided {
    transition-duration: .9s;
    border-radius: 100%;
}

.label-payment {
    margin-bottom: 5px;
    padding: 10px 20px;
    border-radius: 0;
    border: 1px solid #e1e1e1;
    float: left;
    width: 100%;
    background-color: #0053a0;
    color: #ece358;
    font-weight: 800;
    font-size: 30px;
}

.label-payment, .label-status, .label-train {
    font-family: "Raleway", sans-serif;
    text-align: center !important;
    cursor: default;
}

.label-status, .label-train {
    color: #0053a0;
    font-weight: 600;
}

.panel-default2 .panel-heading2 {
    color: #fff;
    background-color: #0053a0 !important;
    font-weight: 800;
    border-color: #0053a0;
    font-family: "Raleway", sans-serif;
}

.table2 > tbody > tr > td, .table2 > tbody > tr > th, .table2 > tfoot > tr > td, .table2 > tfoot > tr > th, .table2 > thead > tr > td, .table2 > thead > tr > th {
    padding: 4px;
    line-height: 1.42857143;
    vertical-align: top;
    border-top: 0px solid #ddd;
}

.font-rail {
    color: #fff;
    font-family: 'Raleway', open-sans;
    font-size: 20px;
    padding: 5px 15px;
}

.font-rail2 {
    color: #fff;
    font-family: 'Raleway', open-sans;
    font-size: 16px;
    padding-top: 15px;
    padding-left: 15px;
}

.rail-way-font {
    font-family: 'Raleway', open-sans;
}

.colors-primary {
    background-color: #0053a0;
    margin-top: 5px;
}

.btn[disabled] {
    cursor: not-allowed;
    filter: alpha(opacity=65);
    -webkit-box-shadow: none;
    box-shadow: none;
    opacity: 1;
}

.btn_book2 {
    background: #0053a0;
    padding: 5px;
    border-radius: 0;
    color: #ffffff;
    font-family: "Open Sans", sans-serif;
    font-weight: 600;
    font-size: 14px;
    letter-spacing: 1px;
    transition: .3s;
    text-transform: capitalize;
}

    .btn_book2:hover, .btn_book2:active {
        background: #ece358;
        transition: .3s;
        color: #0053a0;
        //#636363;
    }

.hot-seat {
    color: #F42536;
}

.normal-seat {
    color: #0053a0;
}

.selected-seat {
    color: #d971f1;
}

.occupied-seat {
    color: #dddddd;
}

#coach .list-inline > li {
    display: inline-block;
    padding-right: 0px;
    padding-left: 0px;
}


.my-tooltip-styling {
    max-width: 300px;
    background-color: #636363;
    color: #fff;
    text-align: center;
    padding: 5px 0;
    border-radius: 6px;
    font-family: "Raleway", sans-serif;
    border: 1px solid #9d9d9d !important;
}


#print-ticket .table > tbody > tr > td {
    padding: 2px;
    line-height: 1.42857143;
    vertical-align: top;
    border-top: 0px solid #ffffff !important;
}

#print-ticket {
    font-family: "Raleway", sans-serif;
}

    #print-ticket .label-status, #print-ticket .label-train, #print-ticket .label-payment {
        color: #000;
        background-color: transparent;
    }

.p-ticketid {
    font-size: 12px;
    font-style: italic;
    margin-bottom: 1px;
    text-align: right;
}

.p-direction {
    font-size: 20px;
    font-weight: 800;
    border: 1px solid #e1e1e1;
    padding: 10px;
    border-radius: 4px;
    text-transform: capitalize;
    background-color: #f5f4f4;
}

.p-font-size1 {
    font-weight: 700;
    font-size: 11px;
}

.p-font-size2 {
    text-align: center;
    font-weight: 700;
    font-size: 15px;
    text-transform: uppercase;
    margin-bottom: 20px;
}

.jarak {
    padding-bottom: 40px;
}

.pull-right2 {
    float: right !important;
}

@media (max-width: 992px) {
    .jarak {
        padding-bottom: 5px;
    }

    .pull-right2 {
        float: none !important;
    }

    .label-payment {
        margin-bottom: 5px;
        padding: 0px;
        border-radius: 0;
        border: 1px solid #e1e1e1;
        float: left;
        width: 100%;
        background-color: #0053a0;
        color: #ece358;
        font-weight: 800;
        font-size: 15px;
    }
}

@media(max-width: 500px) {
    .stepwizard {
        display: table;
        position: relative;
        width: 100%;
    }
}

/*center absolute loader start*/

.square-spin {
    position: fixed;
    z-index: 999;
    height: 2em;
    width: 2em;
    overflow: show;
    margin: auto;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
}

    .square-spin:before {
        content: '';
        display: block;
        position: fixed;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        background-color: rgba(0,0,0,0.3);
    }

    .square-spin > div {
        background: #0053a0;
    }

/*center absolute loader end*/

