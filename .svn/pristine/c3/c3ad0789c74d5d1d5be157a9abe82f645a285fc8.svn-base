                            <span class="caret"></span>
                        </a>
                        <ul class="dropdown-menu">
                            <li><a href="/user/login">Login Here!</a></li>
                        </ul>
                    </li>
                    <!--to redirect register user-->
                    <li class="visible-lg"><span><a href="/user/signup" class="btn btn_order_now">Register User</a></span></li>
                </ul>
            </div>
        </div>
    </nav>
    <!--navigation end here-->
    <!--inner body start from here-->
    <div class="inner-body">
